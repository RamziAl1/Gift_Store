@model Gifts_Store.Models.Category

@{
    ViewData["Title"] = "Gift category details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Gift category details</h1>

<div>
    <h4>Category</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Category name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoryName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
